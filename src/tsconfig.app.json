{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "types": [],
    "baseUrl": ".",
    "paths": {
      "@app/*": ["app/*"],
      "@core/*": ["app/core/*"],
      "@auth/*": ["app/auth/*"],
      "@shared/*": ["app/shared/*"],
      "@assets/*": ["assets*/"],
      "@environments/*": ["environments/*"],
      "@styles/*": ["styles*/"]
    }
  },
  "exclude": ["test.ts", "**/*.spec.ts"]
}
